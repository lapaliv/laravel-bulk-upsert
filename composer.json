{
    "name": "lapaliv/laravel-bulk-upsert",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "bulk",
        "bulk insert",
        "bulk update",
        "bulk upsert",
        "eloquent"
    ],
    "autoload": {
        "psr-4": {
            "Lapaliv\\BulkUpsert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lapaliv\\BulkUpsert\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Lapin Alexey",
            "email": "lapaliv+github@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "nesbot/carbon": "^2.0",
        "illuminate/database": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "8.0.x-dev",
        "vlucas/phpdotenv": "5.5.x-dev",
        "ext-pdo": "*",
        "fakerphp/faker": "^1.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lapaliv\\BulkUpsert\\Providers\\BulkUpsertServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
